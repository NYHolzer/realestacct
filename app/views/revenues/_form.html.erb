<%=form_for acct do |f| %>

<%= render partial:"layouts/error", locals: {obj: acct} %>

<%=f.label :type%>
<%=f.text_field :type, list: "type_autocomplete" %>
    <datalist id="type_autocomplete">
        <%current_user.uniq_buildings.each do |building|%>
            <option value="<%= building.name %>">
        <% end %>
    </datalist>
<br/>
<%=f.label :tenant%>
<%=f.text_field  :tenant%>
<br/>
<%=f.label :building_id%>

<br/>
<%= f.submit submit_text %>
<% end %>