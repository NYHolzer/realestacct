<%=form_for acct do |f| %>

<%= render partial:"layouts/error", locals: {obj: acct} %>

<%=f.label :revenue_type %>
<%=f.select :rev_type, options_for_select(rev_types) %>
<br/>
<%=f.hidden_field :unit_id%>
<%=f.label :amount%>
<%=f.text_field :amount %>
<br/>
<%=f.label :date_paid %>
<%=f.date_field :date%>
<br/>
<%= f.submit submit_text %>
<% end %>