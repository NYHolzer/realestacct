<h1> Tenants </h1>

<table>
    <tr>
        <th>Name </th>
        <th>Apartment Number</th>
        <th>Building</th>
    </tr>
    
    <% @tenants.each do |t| %>
        <tr>
            <td><%=t.tenant%></td>
            <td><%=t.apt_num%></td>
            <td><%=t.building_name%> </td>
        </tr>
    <% end %>
    </table>